mixin information
    ul.information
        h5 Information
            i(class="fas fa-chevron-down")
        each info in ['About US', 'Delivery Information', 'Privacy Policy', 'Terms & Conditions', "Site Map"]
            li: a(href="#")=info

mixin extras
    ul.extras
        h5 Extras
            i(class="fas fa-chevron-down")
        each extra in ['Brands', 'Gift Certificates', 'Affiliate', 'Specials', 'Contact Us']
            li: a(href="#")=extra
